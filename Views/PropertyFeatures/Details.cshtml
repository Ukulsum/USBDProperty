@model USBDProperty.Models.PropertyFeatures

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card">
    <div class="card-body">
        <h5 class="card-title text-center">Property Features Details</h5>
        <hr style="width:100%;" />
        <dl class="row">
            <dt class="col-sm-3">
                @Html.DisplayNameFor(model => model.PropertyFeatureId)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.PropertyFeatureId)
            </dd>
            <dt class="col-sm-3">
                @Html.DisplayNameFor(model => model.PropertyFeatureName)
            </dt>
            <dd class="col-sm-9">
                @Html.DisplayFor(model => model.PropertyFeatureName)
            </dd>
            <dt class="col-sm-3">
                @Html.DisplayNameFor(model => model.FeatureDescription)
            </dt>
            <dd class="col-sm-9">
                @Html.Raw(Model.FeatureDescription)
            </dd>
        </dl>
        <div>
            <a asp-action="Edit" asp-route-id="@Model?.PropertyFeatureId">Edit</a> |
            <a asp-action="Index">Back to List</a>
        </div>
    </div>
</div>

