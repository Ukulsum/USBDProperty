@model USBDProperty.Models.DevelopersorAgent

@{
    ViewData["Title"] = "Delete";
    if(User.IsInRole("Agent"))
    {
        Layout = "~/Views/Shared/_AgentLayout.cshtml";
    }
    else
    {
        // Layout = "~/Views/Shared/_AdminLayout.cshtml";
        Layout = "~/Views/Shared/LayoutAdminLTe.cshtml";
    }
}


<div class="content-wrapper" style="min-height: 1302.12px;">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>Developers or Agents</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a asp-action="Index" asp-controller="DevelopersorAgents">Developers or Agents</a></li>
                        <li class="breadcrumb-item active">Delete</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-md-9">
                                    <h3 class="card-title">Are you sure you want to delete this?</h3>
                                </div>
                            </div>
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <dl class="row">
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.Logo)
                                </dt>
                                <dd class="col-sm-9">
                                    <img class="img-responsive" src="@Url.Content(@Model.Logo)" height="180" width="220" />
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.Banner)

                                </dt>
                                <dd class="col-sm-9">
                                    <img class="img-responsive" src="@Url.Content(@Model.Banner)" height="180" width="220" />
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.CompanyName)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.CompanyName)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.ContactNo)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.ContactNo)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.Email)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.Email)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.Name)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.Name)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.Membership)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.Membership)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.AboutUs)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.Raw(Model.AboutUs)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.CreatedDate)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.CreatedDate)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.CreatedBy)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.CreatedBy)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.UpdateDate)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.UpdateDate)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.UpdateBy)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.UpdateBy)
                                </dd>
                                <dt class="col-sm-3">
                                    @Html.DisplayNameFor(model => model.IsActive)
                                </dt>
                                <dd class="col-sm-9">
                                    @Html.DisplayFor(model => model.IsActive)
                                </dd>
                            </dl>
                        </div>
                        <div class="card-footer">
                            <form asp-action="Delete">
                                <input type="hidden" asp-for="ID" />
                                <input type="submit" value="Delete" class="btn btn-danger" /> |
                                <a asp-action="Index">Back to List</a>
                            </form>
                        </div>
                        <!-- /.card -->
                    </div>
                </div>
            </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
</div>

