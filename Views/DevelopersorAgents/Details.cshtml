@model USBDProperty.Models.DevelopersorAgent

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="card">
    <div class="card-body">
        <h5 class="card-title text-center">Developers Or Agents Details</h5>
        <hr style="width:100%; " />
        <div class="row">
            <div class="col-sm-12 col-md-5">
                <img class="img-responsive mb-2" src="@Url.Content(@Model.Banner)" />
                <div class="row">
                    <div class="col-sm-6 col-md-4">
                        <img class="img-responsive" src="@Url.Content(@Model.Logo)" />
                    </div>
                    <div class="col-sm-6 col-md-8 mt-2">
                        <p>@Model.CompanyName</p>
                        <p>MemberShip: @Model.Membership</p>
                    </div>
                </div>
                <div class="mt-4">
                    <a asp-action="Edit" asp-route-id="@Model?.ID">Edit</a> |
                    <a asp-action="Index">Back to List</a>
                </div>
            </div>
            <div class="col-sm-12 col-md-7">
                <dl class="row">
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.Logo)
                    </dt>
                    <dd class="col-sm-8">
                        <img class="img-responsive" src="@Url.Content(@Model.Logo)" height="100" width="150"/>
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.Banner)
                    </dt>
                    <dd class="col-sm-8">
                        <img class="img-responsive" src="@Url.Content(@Model.Banner)"height="180" width="220" />
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.CompanyName)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.CompanyName)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.ContactNo)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.ContactNo)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.Email)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.Email)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.Name)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.Name)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.Membership)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.Membership)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.AboutUs)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.Raw(Model.AboutUs)
                    </dd>

                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.CreatedDate)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.CreatedDate)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.CreatedBy)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.CreatedBy)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.UpdateDate)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.UpdateDate)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.UpdateBy)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.UpdateBy)
                    </dd>
                    <dt class="col-sm-4">
                        <i class="bi bi-chevron-right"></i>@Html.DisplayNameFor(model => model.IsActive)
                    </dt>
                    <dd class="col-sm-8">
                        @Html.DisplayFor(model => model.IsActive)
                    </dd>
                </dl>
            </div>
        </div>
    </div>
</div>



