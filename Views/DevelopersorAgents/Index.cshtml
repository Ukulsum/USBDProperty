@model IEnumerable<USBDProperty.Models.DevelopersorAgent>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@{
    @User.Identity.Name
}
@* <main id="main"> *@
<div class="card">
    <div class="card-body">

        <div class="row">
            <div class="col-md-9">
                <h5 class="card-title">All Developers</h5>
            </div>
            <div class="col-md-3">
                <h5 class="card-title"><a asp-action="Create">Create New</a></h5>
            </div>
        </div>

        <table class="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Logo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Banner)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CompanyName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ContactNo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Email)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Name)
                    </th>
                    @*  <th>
                        @Html.DisplayNameFor(model => model.PostedBy)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.CreatedDate)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.CreatedBy)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.UpdateDate)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.UpdateBy)
                        </th>
                        <th>
                        @Html.DisplayNameFor(model => model.IsActive)
                        </th> *@
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @*  @Html.DisplayFor(modelItem => item.Logo) *@
                            <img src="@Url.Content(item.Logo)" alt="@item.CompanyName" height="80" width="120" />
                        </td>
                        <td>
                            @* @Html.DisplayFor(modelItem => item.Banner) *@
                            <img src="@Url.Content(item.Banner)" alt="@item.CompanyName" height="180" width="220" />
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CompanyName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ContactNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        @*   <td>
                            @Html.DisplayFor(modelItem => item.PostedBy)
                            </td>
                            <td>
                            @Html.DisplayFor(modelItem => item.CreatedDate)
                            </td>
                            <td>
                            @Html.DisplayFor(modelItem => item.CreatedBy)
                            </td>
                            <td>
                            @Html.DisplayFor(modelItem => item.UpdateDate)
                            </td>
                            <td>
                            @Html.DisplayFor(modelItem => item.UpdateBy)
                            </td>
                            <td>
                            @Html.DisplayFor(modelItem => item.IsActive)
                            </td> *@
                        <td>
                            <a asp-action="Edit" asp-route-id="@item.ID"><i class="bi bi-pencil-square"></i></a> |
                            <a asp-action="Details" asp-route-id="@item.ID"><i class="bi bi-eye-fill"></i></a> |
                            <a asp-action="Delete" asp-route-id="@item.ID"><i class="bi bi-trash"></i></a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
@* </main> *@